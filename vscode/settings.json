{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.formatOnType": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "apc.font.family": "Zed Mono Extended",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": true,
  "editor.fontFamily": "Zed Mono Extended",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.fontWeight": 500,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.overviewRulerBorder": false,
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "none",
  "editor.roundedSelection": true,
  "editor.rulers": [],
  "editor.stickyScroll.enabled": true,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "[Sequoia Monochrome]": {
      "textMateRules": [
        {
          "scope": [
            "entity.other.attribute-name",
            "keyword.control",
            "keyword.operator.new",
            "meta.object-literal.key",
            "storage.modifier",
            "storage.type"
          ],
          "settings": {
            "foreground": "#10a694"
          }
        },
        {
          "scope": [
            "string",
            "keyword.operator.assignment",
            "keyword.operator.arithmetic",
            "keyword.operator.bitwise",
            "keyword.operator.comparison",
            "keyword.operator.logical",
            "meta.arrow",
            "punctuation.definition.tag",
            "storage.type.function.arrow"
          ],
          "settings": {
            "foreground": "#f0a92f"
          }
        },
        {
          "scope": [
            "keyword.operator.optional",
            "meta",
            "punctuation.accessor",
            "punctuation.separator",
            "punctuation.terminator",
            "source.python",
            "support.variable",
            "variable",
            "variable.other",
            "variable.other.object",
            "variable.other.readwrite.alias"
          ],
          "settings": {
            "foreground": "#f7f7f8"
          }
        },
        {
          "scope": ["meta.function-call", "entity.name.function"],
          "settings": {
            "foreground": "#fee56d"
          }
        },
        {
          "scope": [
            "entity.name.type",
            "support.class",
            "support.type.primitive"
          ],
          "settings": {
            "foreground": "#07e7c6"
          }
        },
        {
          "scope": ["constant"],
          "settings": {
            "foreground": "#96df72"
          }
        }
      ]
    }
  },
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "python.testing.pytestEnabled": true,
  "telemetry.telemetryLevel": "off",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.fontSize": 18,
  "typescript.tsserver.log": "verbose",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.titleBarStyle": "native",
  "workbench.activityBar.location": "top",
  "workbench.colorCustomizations": {
    "activityBarBadge.background": "#07e7c6",
    "activityBarBadge.foreground": "#000000",
    "editor.background": "#1e2025",
    "editorBracketHighlight.foreground1": "#f7f7f8",
    "editorBracketHighlight.foreground2": "#f7f7f8",
    "editorBracketHighlight.foreground3": "#f7f7f8",
    "editorBracketHighlight.foreground4": "#f7f7f8",
    "editorBracketHighlight.foreground5": "#f7f7f8",
    "editorBracketHighlight.foreground6": "#f7f7f8",
    "editorBracketHighlight.unexpectedBracket.foreground": "#f7f7f8",
    "editorError.foreground": "#da0d51"
  },
  "workbench.colorTheme": "Sequoia Monochrome",
  "workbench.editor.wrapTabs": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.sideBar.location": "right",
  "editor.guides.indentation": false
}
