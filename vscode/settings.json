{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.formatOnType": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "apc.font.family": "Geist Mono",
  "apc.monospace.font.family": "Geist Mono",
  "apc.parts.font.family": {
    "sidebar": "Geist Mono",
    "titlebar": "Geist Mono",
    "activityBar": "Geist Mono",
    "panel": "Geist Mono",
    "tabs": "Geist Mono",
    "statusbar": "Geist Mono",
    "settings-body": "Geist Mono",
    "extension-editor": "Geist Mono",
    "monaco-menu": "Geist Mono"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": true,
  "editor.fontFamily": "Geist Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.fontWeight": 500,
  "editor.formatOnSave": true,
  "editor.guides.indentation": false,
  "editor.inlineSuggest.enabled": true,

  "editor.minimap.enabled": false,
  "editor.overviewRulerBorder": false,
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "none",
  "editor.roundedSelection": true,

  "editor.stickyScroll.enabled": true,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "[Sequoia Monochrome]": {
      "textMateRules": [
        {
          "scope": [
            "entity.other.attribute-name",
            "keyword.control",
            "keyword.operator.new",
            "keyword.operator.expression.in",
            "keyword.operator.expression.typeof",
            "meta.embedded.expression",
            "meta.object-literal.key",
            "storage.modifier",
            "storage.type"
          ],
          "settings": {
            "foreground": "#10a694"
          }
        },
        {
          "scope": [
            "string",
            "keyword.operator.assignment",
            "keyword.operator.arithmetic",
            "keyword.operator.bitwise",
            "keyword.operator.comparison",
            "keyword.operator.logical",
            "markup.inline.raw.string.markdown",
            "meta.arrow",
            "punctuation.definition.tag",
            "punctuation.definition.string.template",
            "storage.type.function.arrow"
          ],
          "settings": {
            "foreground": "#f0a92f"
          }
        },
        {
          "scope": [
            "keyword.operator.optional",

            "punctuation.accessor",
            "punctuation.definition.keyword",
            "punctuation.separator",
            "punctuation.support.type.property-name",
            "punctuation.terminator",
            "source.python",
            "support.variable",
            "variable",
            "variable.other",
            "variable.other.object",
            "variable.other.readwrite.alias"
          ],
          "settings": {
            "foreground": "#f7f7f8"
          }
        },
        {
          "scope": ["meta.function-call", "entity.name.function"],
          "settings": {
            "foreground": "#fee56d"
          }
        },
        {
          "scope": [
            "constant.language.json.comments",
            "constant.numeric.json.comments",
            "entity.name.type",
            "entity.name.tag",
            "entity.other.inherited-class",
            "punctuation.definition.template-expression",
            "support.class",
            "support.type.primitive",
            "variable.language.this"
          ],
          "settings": {
            "foreground": "#20E3B2"
          }
        },
        {
          "scope": ["constant", "variable.other.constant.property"],
          "settings": {
            "foreground": "#20e3b2"
          }
        }
      ]
    }
  },
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "python.testing.pytestEnabled": true,
  "telemetry.telemetryLevel": "off",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.fontFamily": "RobotoMono Nerd Font Mono",
  "terminal.integrated.fontSize": 18,
  "typescript.tsserver.log": "off",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.titleBarStyle": "native",
  "workbench.activityBar.location": "top",
  "workbench.colorCustomizations": {
    "activityBar.activeBorder": "#868690",
    "activityBar.background": "#22242b",
    "activityBar.dropBorder": "#131317",
    "activityBar.foreground": "#868690",
    "activityBar.inactiveForeground": "#575861",
    "activityBarBadge.background": "#07e7c6",
    "activityBarBadge.foreground": "#000000",
    "breadcrumb.background": "#1e2025",
    "breadcrumb.foreground": "#868690",
    "debugIcon.breakpointForeground": "#da0d51",
    "diffEditor.insertedTextBackground": "#20E3B244",
    "diffEditor.removedTextBackground": "#f0622f44",
    "editor.background": "#1e2025",
    "editor.selectionBackground": "#1a3b3a",
    "editor.selectionHighlightBackground": "#1a3b3a",
    "editorBracketHighlight.foreground1": "#f7f7f8",
    "editorBracketHighlight.foreground2": "#f7f7f8",
    "editorBracketHighlight.foreground3": "#f7f7f8",
    "editorBracketHighlight.foreground4": "#f7f7f8",
    "editorBracketHighlight.foreground5": "#f7f7f8",
    "editorBracketHighlight.foreground6": "#f7f7f8",
    "editorBracketHighlight.unexpectedBracket.foreground": "#f7f7f8",
    "editorError.foreground": "#da0d51",
    "editorOverviewRuler.background": "#1e2025",
    "editorOverviewRuler.errorForeground": "#da0d51",
    "errorForeground": "#da0d51",
    "list.errorForeground": "#da0d51",
    "list.hoverBackground": "#303032",
    "list.activeSelectionForeground": "#fee56d",
    "list.focusForeground": "#fee56d",
    "sideBar.background": "#22242b",
    "sideBar.foreground": "#aca8ad",
    "tab.activeBackground": "#47494d",
    "tab.activeForeground": "#f7f7f8",

    "terminal.ansiBlack": "#21222c",
    "terminal.ansiBlue": "#bd93f9",
    "terminal.ansiBrightBlack": "#8d92ff",
    "terminal.ansiBrightBlue": "#bd93f9",
    "terminal.ansiBrightCyan": "#2cccff",
    "terminal.ansiBrightGreen": "#20E3B2",
    "terminal.ansiBrightMagenta": "#ff6bcb",
    "terminal.ansiBrightRed": "#ff6e6e",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.ansiBrightYellow": "#EAC394",
    "terminal.ansiCyan": "#8be9fd",
    "terminal.ansiGreen": "#20e3b2",
    "terminal.ansiMagenta": "#ff6bcb",
    "terminal.ansiRed": "#ff5555",
    "terminal.ansiWhite": "#f8f8f2",
    "terminal.ansiYellow": "#fde181",
    "terminalCursor.background": "#868690",
    "terminalCursor.foreground": "#43444D"
  },
  "workbench.colorTheme": "Sequoia Monochrome",
  "workbench.editor.wrapTabs": true,
  "workbench.iconTheme": "city-lights-icons-vsc",
  "workbench.sideBar.location": "right"
}
